<select class="form-control float-right" style="max-width: 100px;margin-right: 30px;">
  <option value="all">All</option>
    <option value="one">Grey</option>
    <option value="two">Green</option>
    <option value="three">Red</option>
</select>
<br>
<% campusAmbassadors.forEach(function(campusAmbassador){ %>
<br>
<div class="card">
  <% var color = campusAmbassador.tag; %>
   <% var name = campusAmbassador.id+" CampusAmbassador" %>
  <div class="card-header" style="background: <%= color %>;" name="<%= name %>">
    <%= campusAmbassador.time.getDate() %>/<%= campusAmbassador.time.getMonth() %>/<%= campusAmbassador.time.getFullYear() %>
    &nbsp <%= campusAmbassador.time.getHours() %>:<%= campusAmbassador.time.getMinutes() %>
    <div class="circle-grey float-right" style="margin-left: 7px;margin-top: 5px;">
    </div>
     <div class="circle-green float-right" style="margin-left: 7px;margin-top: 5px;">
    </div>
     <div class="circle-red float-right" style="margin-left: 7px;margin-top: 5px;">
    </div>
  </div>
  <div class="card-block">
  	<ul class="list-group list-group-flush">
    <li class="list-group-item">Name:&nbsp<strong><%= campusAmbassador.name %></strong></li>
    <li class="list-group-item">College:&nbsp<%= campusAmbassador.college %></li>
    <li class="list-group-item">Contact:&nbsp<%= campusAmbassador.number %></li>
     <li class="list-group-item">Email:&nbsp<%= campusAmbassador.email %></li>
    <li class="list-group-item">Year:&nbsp<%= campusAmbassador.year %></li>
    <li class="list-group-item">Area of study:&nbsp<%= campusAmbassador.area %></li>
    <li class="list-group-item">Why do you wish to be a campus ambassador:&nbsp<strong><%= campusAmbassador.why %></strong></li>
  </ul>
  </div>
</div>
<br>
<% }); %>