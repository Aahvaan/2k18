<select class="form-control float-right" style="max-width: 100px;margin-right: 30px;">
  <option value="all">All</option>
    <option value="one">Grey</option>
    <option value="two">Green</option>
    <option value="three">Red</option>
</select>
<br>
<% teams.forEach(function(team){ %>
<br>
<div class="card">
  <% var color = team.tag; %>
   <% var name = team.id+" Team" %>
  <div class="card-header" style="background: <%= color %>;" name="<%= name %>">
    <%= team.time.getDate() %>/<%= team.time.getMonth() %>/<%= team.time.getFullYear() %>
    &nbsp <%= team.time.getHours() %>:<%= team.time.getMinutes() %>
     <div class="circle-grey float-right" style="margin-left: 7px;margin-top: 5px;">
    </div>
     <div class="circle-green float-right" style="margin-left: 7px;margin-top: 5px;">
    </div>
     <div class="circle-red float-right" style="margin-left: 7px;margin-top: 5px;">
    </div>
  </div>
  <div class="card-block">
  	<ul class="list-group list-group-flush">
    <li class="list-group-item">Contingent Leader Name:&nbsp<strong><a href=""><%= team.leader.name %></strong></li></a>
     <li class="list-group-item">Team Captain Name:&nbsp<strong><%= team.captain %></strong></li>
      <li class="list-group-item">Contact:&nbsp<%= team.contact %></li>
    <li class="list-group-item">College:&nbsp<%= team.college %></li>
     <li class="list-group-item">Sport:&nbsp<%= team.sport %></li>
     <li class="list-group-item">Number of Players:&nbsp<%= team.number_of_players %></li>
    <% for(var i=0;i<team.number_of_players;i++){ %>
     <li class="list-group-item">Player&nbsp<%= (i+1) %>&nbsp Name:&nbsp<%= team.players[i] %></li>
    <% } %>
  </ul>
  </div>
</div>
<br>
<% }); %>